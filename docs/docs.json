{
  "$schema": "https://mintlify.com/docs.json",
  "theme": "aspen",
  "name": "Docs",
  "colors": {
    "primary": "#3100FF",
    "light": "#0C83F2",
    "dark": "#0C83F2"
  },
  "favicon": "/favicon.ico",
  "navigation": {
    "tabs": [
      {
        "tab": "Documentation",
        "groups": [
          {
            "group": "Introduction",
            "pages": ["index", "introduction/welcome"]
          },
          {
            "group": "Getting Started",
            "pages": [
              "quickstart",
              "getting-started/installing-codinit",
              "getting-started/select-your-model",
              "getting-started/your-first-project"
            ]
          },
          {
            "group": "Features",
            "pages": [
              "features/overview",
              {
                "group": "Development Tools",
                "pages": [
                  "features/development/workbench",
                  "features/development/code-editor",
                  "features/development/terminal",
                  "features/development/webcontainer",
                  "features/development/developers"
                ]
              }
            ]
          },
          {
            "group": "Model Configuration",
            "pages": ["model-config/context-windows", "model-config/model-comparison"]
          },
          {
            "group": "Cloud Providers",
            "pages": [
              "providers/provider-guide",
              {
                "group": "Major AI Providers",
                "pages": ["providers/anthropic", "providers/openai", "providers/google", "providers/mistral-ai"]
              },
              {
                "group": "Alternative Providers",
                "pages": [
                  "providers/groq",
                  "providers/deepseek",
                  "providers/fireworks",
                  "providers/openrouter",
                  "providers/xai-grok"
                ]
              },
              {
                "group": "Local Providers",
                "pages": ["providers/ollama"]
              }
            ]
          },
          {
            "group": "Running Models Locally",
            "pages": ["running-models-locally/local-model-setup", "running-models-locally/lm-studio"]
          },
          {
            "group": "Prompting",
            "pages": ["prompting/prompt-engineering-guide", "prompting/prompting-effectively"]
          }
        ]
      },
      {
        "tab": "Advanced Features",
        "groups": [
          {
            "group": "Essentials",
            "pages": ["essentials/project-templates", "essentials/ai-chat-commands", "essentials/customization"]
          },
          {
            "group": "Integrations",
            "pages": [
              {
                "group": "Version Control",
                "pages": ["integrations/git"]
              },
              {
                "group": "Backend Services",
                "pages": ["integrations/supabase"]
              },
              {
                "group": "Deployment Platforms",
                "pages": ["integrations/deployments", "integrations/netlify", "integrations/vercel"]
              }
            ]
          },
          {
            "group": "MCP Integration",
            "pages": ["mcp-integration/mcp"]
          }
        ]
      },
      {
        "tab": "Help",
        "groups": [
          {
            "group": "Help & Documentation",
            "pages": ["frequently-asked-questions", "troubleshooting"]
          }
        ]
      }
    ],
    "global": {
      "anchors": [
        {
          "anchor": "Blog",
          "href": "https://codinit.dev/blog",
          "icon": "newspaper"
        },
        {
          "anchor": "Releases",
          "href": "https://github.com/Gerome-Elassaad/codinit-app/releases",
          "icon": "github"
        }
      ]
    }
  },
  "logo": {
    "light": "/logo/logo-dark.png",
    "dark": "/logo/icon.webp"
  },
  "navbar": {
    "links": [
      {
        "label": "Issues",
        "href": "https://github.com/Gerome-Elassaad/codinit-app"
      }
    ],
    "primary": {
      "type": "button",
      "label": "Download",
      "href": "https://codinit.dev/download"
    }
  },
  "contextual": {
    "options": ["copy", "view", "chatgpt", "claude", "perplexity", "mcp", "cursor", "vscode"]
  },
  "footer": {
    "socials": {
      "github": "https://github.com/codinit-dev"
    }
  },
  "redirects": [
    {
      "source": "/getting-started",
      "destination": "/quickstart"
    },
    {
      "source": "/getting-started/developer-docs",
      "destination": "/features/development/developers"
    },
    {
      "source": "/getting-started/quickstart",
      "destination": "/quickstart"
    },
    {
      "source": "/getting-started/teams",
      "destination": "/getting-started/your-first-project"
    },
    {
      "source": "/integrations/github",
      "destination": "/integrations/git"
    },
    {
      "source": "/integrations/plugins",
      "destination": "/mcp-integration/mcp"
    },
    {
      "source": "/models/anthropic",
      "destination": "/providers/anthropic"
    },
    {
      "source": "/models/deepseek",
      "destination": "/providers/deepseek"
    },
    {
      "source": "/models/gcp-vertex-ai",
      "destination": "/providers/google"
    },
    {
      "source": "/models/gemini",
      "destination": "/providers/google"
    },
    {
      "source": "/models/groq",
      "destination": "/providers/groq"
    },
    {
      "source": "/models/mistral",
      "destination": "/providers/mistral-ai"
    },
    {
      "source": "/models/ollama",
      "destination": "/providers/ollama"
    },
    {
      "source": "/models/openai",
      "destination": "/providers/openai"
    },
    {
      "source": "/providers/togetherai",
      "destination": "/providers/openrouter"
    }
  ]
}
